		Sub RemoveFavorite()
		  Dim I As Integer  
		  If CurrentItemID = -1 Then Return 'No Item Text, skip
		  For I = 0 To FavCount - 1
		    If Favorites(I).Lowercase = Data.Items.CellTextAt(CurrentItemID, Data.GetDBHeader("TitleName")).Lowercase  Then 
